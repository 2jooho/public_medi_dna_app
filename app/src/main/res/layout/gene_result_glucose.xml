<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginLeft="40dp"
    android:layout_marginTop="20dp">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <FrameLayout
            android:layout_width="150dp"
            android:layout_height="180dp">

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="0sp"
            android:scaleType="centerCrop"
            android:src="@drawable/disease_shape"/>
            <TextView
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="45dp"
                android:background="@drawable/disease_shape2"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="35dp"
                android:text="합병증"
                android:textColor="#cc000000"
                android:textSize="22dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="85dp"
                android:text="지방간 대장암 유방암 요실금 심근경색"
                android:id="@+id/complication"
                android:maxLines="2"
                android:ellipsize="end"
                android:textColor="#e6000000"
                android:textSize="22dp" />
            <TextView
            android:layout_width="28dp"
            android:layout_height="12dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="150dp"
            android:text="합병증"
            android:textAlignment="center"
            android:background="@drawable/disease_shape3_min"
            android:textColor="#e88579"
            android:textSize="8dp" />
            <TextView
                android:layout_width="20dp"
                android:layout_height="12dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="150dp"
                android:text="●●●"
                android:textAlignment="center"
                android:background="@drawable/disease_shape3_min"
                android:textColor="#e88579"
                android:textSize="8dp" />
        </FrameLayout>
        <FrameLayout
            android:layout_width="150dp"
            android:layout_height="180dp"
            android:layout_marginLeft="20dp">

            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="0sp"
                android:scaleType="centerCrop"
                android:src="@drawable/disease_shape"/>
            <TextView
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="45dp"
                android:background="@drawable/symptom_shape2"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="35dp"
                android:text="이상증상"
                android:textColor="#cc000000"
                android:textSize="22dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="85dp"
                android:maxLines="2"
                android:id="@+id/sym"
                android:ellipsize="end"
                android:text="월경이상 허리통증 우울감 당뇨 고혈압"
                android:textColor="#e6000000"
                android:textSize="22dp" />

            <TextView
                android:layout_width="28dp"
                android:layout_height="12dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="150dp"
                android:text="이상증상"
                android:textAlignment="center"
                android:background="@drawable/symptom_shape_min"
                android:textColor="#c8900f"
                android:textSize="8dp" />
            <TextView
